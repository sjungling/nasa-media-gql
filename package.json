{
  "name": "spaceapi-media-graphql",
  "version": "1.0.0",
  "author": "Scott Jungling <scott.jungling@gmail.com>",
  "license": "MIT",
  "scripts": {
    "prebuild": "yarn generate",
    "prestart": "yarn generate",
    "build": "netlify-lambda build src/graphql --config ./webpack.config.js",
    "start": "npx netlify-lambda -p 8888 serve src/graphql --config ./webpack.config.js",
    "dev": "npx netlify dev",
    "generate": "graphql-codegen --config codegen.yml",
    "format": "prettier -w 'src/**/*.{ts,tsx,graphql,yml,json}' '**/*.md'"
  },
  "devDependencies": {
    "@apollo/federation": "0.20.7",
    "@babel/plugin-proposal-class-properties": "7.18.6",
    "@babel/plugin-proposal-object-rest-spread": "7.18.6",
    "@babel/plugin-transform-object-assign": "7.12.1",
    "@babel/preset-env": "7.18.6",
    "@babel/preset-typescript": "7.12.7",
    "@graphql-codegen/cli": "2.13.5",
    "@graphql-codegen/typescript": "2.4.11",
    "@graphql-codegen/typescript-resolvers": "2.6.6",
    "@graphql-tools/webpack-loader": "6.6.1",
    "apollo-datasource-rest": "3.6.1",
    "apollo-server-lambda": "2.19.0",
    "babel-cli": "6.26.0",
    "clean-webpack-plugin": "3.0.0",
    "copy-webpack-plugin": "6.4.0",
    "dotenv": "16.0.3",
    "graphql": "16.6.0",
    "netlify-lambda": "2.0.15",
    "netlify-plugin-contextual-env": "0.3.0",
    "prettier": "2.4.1",
    "typescript": "4.7.4",
    "webpack": "5.10.1",
    "webpack-cli": "4.2.0",
    "webpack-dev-server": "3.11.0",
    "webpack-node-externals": "3.0.0"
  }
}
